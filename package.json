{
  "name": "list-share",
  "description": "Share your lists, track your friends' lists, collaborate!",
  "author": "C.J. Winslow <whoaa512@gmail.com> (http://github.com/whoaa512)",
  "license": "MIT",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/whoaa512/list-share"
  },
  "homepage": "https://github.com/whoaa512/list-share",
  "private": true,
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrent --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
    "start-prod": "node ./node_modules/better-npm-run start-prod",
    "start-prod-api": "node ./node_modules/better-npm-run start-prod-api",
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "prep-dbs": "touch ./api/list-share-db.json",
    "postinstall": "npm run prep-dbs && npm run prod-build",
    "prod-build": "NODE_ENV=production webpack --display-error-details --config webpack/prod.config.js",
    "lint": "standard",
    "start-dev": "node ./node_modules/better-npm-run start-dev",
    "start-dev-api": "node ./node_modules/better-npm-run start-dev-api",
    "watch-client": "node ./node_modules/better-npm-run watch-client",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-dev-api\"",
    "test": "karma start",
    "test-node": "./node_modules/mocha/bin/mocha ./api/**/__tests__/*-test.js --compilers js:babel-core/register"
  },
  "pre-commit": "lint",
  "standard": {
    "parser": "babel-eslint",
    "global": [
      "__CLIENT__",
      "__SERVER__",
      "__DISABLE_SSR__",
      "__DEVELOPMENT__",
      "__DEVTOOLS__",
      "socket",
      "webpackIsomorphicTools",
      "after",
      "afterEach",
      "before",
      "beforeEach",
      "describe",
      "it"
    ],
    "ignore": [
      "build",
      "static",
      "test-build"
    ]
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3030
      }
    },
    "start-prod-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "production",
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js | bunyan --color",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "start-dev-api": {
      "command": "node ./bin/api.js | bunyan --color",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "APIPORT": 3030
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    }
  },
  "dependencies": {
    "array-uniq": "^1.0.2",
    "babel": "~5.8.29",
    "bcrypt": "^0.8.5",
    "bluebird": "^3.0.5",
    "body-parser": "^1.14.1",
    "bunyan": "^1.5.1",
    "compression": "^1.6.0",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "history": "^1.13.0",
    "hoist-non-react-statics": "^1.0.3",
    "http-proxy": "^1.12.0",
    "lodash.clonedeep": "^3.0.2",
    "lodash.compact": "^3.0.0",
    "lodash.find": "^3.2.1",
    "lodash.get": "^3.7.0",
    "lodash.includes": "^3.1.3",
    "lodash.indexby": "^3.1.1",
    "lodash.isempty": "^3.0.4",
    "lodash.merge": "^3.3.2",
    "lodash.omit": "^3.1.0",
    "lokijs": "^1.3.10",
    "lru-memoize": "^1.0.0",
    "map-props": "^1.0.0",
    "multireducer": "^1.0.2",
    "piping": "^0.3.0",
    "pretty-error": "^1.2.0",
    "pui-react-dividers": "^3.0.0-alpha.1",
    "query-string": "^3.0.0",
    "react": "^0.14.2",
    "react-bootstrap": "^0.28.1",
    "react-document-meta": "^2.0.0",
    "react-dom": "^0.14.1",
    "react-inline-css": "^2.0.0",
    "react-notification-system": "^0.2.6",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0-rc3",
    "react-router-bootstrap": "^0.19.3",
    "redux": "^3.0.4",
    "redux-form": "^3.0.0",
    "redux-logger": "^2.0.4",
    "scroll-behavior": "^0.3.0",
    "serialize-javascript": "^1.1.2",
    "serve-favicon": "^2.3.0",
    "session-file-store": "0.0.22",
    "socket.io": "^1.3.7",
    "socket.io-client": "^1.3.7",
    "superagent": "^1.4.0",
    "url-loader": "^0.5.6",
    "uuid": "^2.0.1",
    "webpack-isomorphic-tools": "^2.2.4"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "~5.8.33",
    "babel-eslint": "^4.1.5",
    "babel-loader": "~5.3.3",
    "babel-plugin-react-transform": "~1.1.1",
    "babel-runtime": "~5.8.29",
    "better-npm-run": "^0.0.3",
    "bootstrap-sass": "^3.3.5",
    "bootstrap-sass-loader": "^1.0.9",
    "chai": "^3.3.0",
    "clean-webpack-plugin": "^0.1.3",
    "concurrently": "^0.1.1",
    "css-loader": "^0.20.1",
    "eslint": "1.8.0",
    "eslint-config-standard": "^4.4.0",
    "eslint-config-standard-react": "^1.2.1",
    "eslint-plugin-import": "^0.8.0",
    "eslint-plugin-react": "^3.9.0",
    "eslint-plugin-standard": "^1.3.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "font-awesome": "^4.4.0",
    "font-awesome-webpack": "0.0.4",
    "json-loader": "^0.5.3",
    "karma": "^0.13.10",
    "karma-cli": "^0.1.1",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "mocha": "^2.3.3",
    "node-sass": "^3.3.3",
    "phantomjs": "^1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "pre-commit": "^1.1.2",
    "react-a11y": "^0.2.6",
    "react-addons-test-utils": "^0.14.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "redux-router": "1.0.0-beta3",
    "sass-loader": "^3.0.0",
    "standard": "^5.4.1",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.4.1"
  },
  "engines": {
    "node": "4.1.1"
  }
}
